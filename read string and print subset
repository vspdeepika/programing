#include<stdio.h>
#include<string.h>
int main()
{
	char temp,s[100],t[100];
	int i,n,j,m,c=0;
	gets(s);
	gets(t);
	n=strlen(s);
	m=strlen(t);
	for(i=0;i<n-1;i++)
	{
		for(j=i+1;j<n;j++)
		{
			if(s[i]>s[j])
			{
				temp=s[i];
				s[i]=s[j];
				s[j]=temp;
			}
		}
	}
	for(i=0;i<m-1;i++)
	{
		for(j=i+1;j<m;j++)
		{
			if(t[i]>t[j])
			{
				temp=t[i];
				t[i]=t[j];
				t[j]=temp;
			}
		}
	}
	puts(s);
	puts(t);
	for(i=0;s[i]!='\0';i++)
	{
		if(s[i]==t[i])
		c++;
	}
	if(c==n || c==m)
	printf("ANAGRAM");
	else
	printf("NOT ANAGRAM");
}
